<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bitc.cms.hdata.ContentPlaceClt" table="content_place_clt" catalog="bitc">
        <id name="cltId" column="clt_id"/>
        <property name="contentId" column="content_id"/>
        <property name="title" column="title"/>
        <property name="abstract_" column="abstract"/>
        <property name="body" column="body"/>
        <property name="info" column="info"/>
        <property name="smallImage" column="small_image"/>
        <property name="bigImage" column="big_image"/>
        <property name="url" column="url"/>
        <property name="urlLabel" column="url_label"/>
        <property name="culture" column="culture"/>
        <property name="openCount" column="open_count"/>
        <property name="commentCount" column="comment_count"/>
        <property name="sourceTitle" column="source_title"/>
        <property name="sourceAbstract" column="source_abstract"/>
        <property name="sourceBody" column="source_body"/>
        <property name="sourceInfo" column="source_info"/>
        <property name="sourceId" column="source_id"/>
    </class>
</hibernate-mapping>